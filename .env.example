# Environment Configuration
NODE_ENV=development

# Server Configuration
PORT=5000

# Google Gemini AI
GOOGLE_API_KEY=your_google_api_key_here

# PostgreSQL Database (automatically provided by Replit)
DATABASE_URL=your_database_url_here
PGPORT=5432
PGUSER=your_pg_user
PGPASSWORD=your_pg_password
PGDATABASE=your_pg_database
PGHOST=your_pg_host

# Replit Configuration (automatically set)
REPLIT_DEPLOYMENT=false
REPL_ID=your_repl_id_here
REPLIT_DOMAINS=your_replit_domains_here

# Authentication (optional for Replit Auth)
SESSION_SECRET=your_session_secret_here
ISSUER_URL=https://replit.com/oidc